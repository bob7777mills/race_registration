<h1>Listing organizers</h1>

<table>
  <tr>
    <th>name</th>
    <th>website</th>
    <th>phone</th>
    <th>cell</th>
    <th>email</th>
    <th>contact</th>
    <th>address1</th>
    <th>address2</th>
    <th>city</th>
    <th>state</th>
    <th>zip</th>
    <th></th>
    <th></th>
    <th></th>


  </tr>

<% @organizers.each do |organizer| %>
  <tr>
  <td><%= organizer.name %></td>
  <td><%= organizer.website %></td>
  <td><%= organizer.phone_num %></td>
  <td><%= organizer.cell_num %></td>
  <td><%= organizer.email_address %></td>
  <td><%= organizer.contact_name %></td>
  <td><%= organizer.address1 %></td>
  <td><%= organizer.address2 %></td>
  <td><%= organizer.city %></td>
  <td><%= organizer.state_cd %></td>
  <td><%= organizer.zip_cd %></td>

  <td><%= link_to 'Show', organizer %></td>
    <td><%= link_to 'Edit', edit_organizer_path(organizer) %></td>
    <td><%= link_to 'Destroy', organizer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Organizer', new_organizer_path %>
