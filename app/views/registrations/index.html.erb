<h1>Listing registrations</h1>

<table>
  <tr>
    <th>user</th>
    <th>age group</th>
    <th>pricing</th>
    <th>registered_at</th>
    <th>waiver signed</th>
    <th>waiver signed under 18</th>
    <th>confirm cd</th>
    <th>tshirt size</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @registrations.each do |registration| %>
  <tr>
    <td><%= registration.user_id %></td>
    <td><%= registration.age_group_detail_id %></td>
    <td><%= registration.pricing_id %></td>
    <td><%= registration.registered_at %></td>
    <td><%= registration.waiver_signed %></td>
    <td><%= registration.waiver_signed_under_18 %></td>
    <td><%= registration.confirm_code %></td>
    <td><%= registration.tshirt_size %></td>

    <td><%= link_to 'Show', registration %></td>
    <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
    <td><%= link_to 'Destroy', registration, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Registration', new_registration_path %>
