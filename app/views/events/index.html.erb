<h1>Listing events</h1>

<table>
  <tr>
    <th>organizer_id</th>
    <th>name</th>
    <th>description</th>
    <th>starts_on</th>
    <th>ends_on</th>
    <th>address</th>
    <th>city</th>
    <th>state_cd</th>
    <th>zip_cd</th>
    <th></th>
    <th></th>
    <th></th>
   </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.organizer_id %></td>
    <td><%= event.name %></td>
    <td><%= event.description %></td>
    <td><%= event.starts_on %></td>
    <td><%= event.ends_on %></td>
    <td><%= event.address %></td>
    <td><%= event.city %></td>
    <td><%= event.state_cd %></td>
    <td><%= event.zip_cd %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
